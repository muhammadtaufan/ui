{
    "compilerOptions": {
      "paths": {
        "@/*": ["./src/*"],
        "@/components/*": ["./src/components/*"],
        "@/container/*": ["./src/container/*"],
        "@/assets/*": ["./src/assets/*"],
      },
      "typeRoots": ["./typings/", "./node_modules/@types/"]
    },
    "include": ["./src", "./typings", "./vite.config.js"],
    "exclude": ["node_modules"]
  }
  